{
  "name": "acf-extension",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/acf-extension/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/acf-extension",
        "extractCss": true,
        "main": "apps/acf-extension/src/background/index.ts",
        "outputFileName": "background.js",
        "additionalEntryPoints": [
          {
            "entryName": "content_scripts",
            "entryPath": "apps/acf-extension/src/content_scripts/index.ts"
          },
          {
            "entryName": "wizard",
            "entryPath": "apps/acf-extension/src/wizard/index.ts"
          },
          {
            "entryName": "wizard-popup",
            "entryPath": "apps/acf-extension/src/wizard/popup/wizard-popup.ts"
          },
          {
            "entryName": "wizard-popup-styles",
            "entryPath": "apps/acf-extension/src/wizard/popup/wizard-popup.scss"
          }
        ],
        "styles": ["apps/acf-extension/src/wizard/popup/wizard-popup.scss"],
        "tsConfig": "apps/acf-extension/tsconfig.app.json",
        "isolatedConfig": true,
        "webpackConfig": "apps/acf-extension/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "acf-extension:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "acf-extension:build:development"
        },
        "production": {
          "buildTarget": "acf-extension:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/acf-extension/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/acf-extension/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
