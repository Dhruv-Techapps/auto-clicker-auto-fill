{
  "name": "acf-extension",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/acf-extension/src",
  "tags": ["type:extension", "scope:acf"],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/acf-extension",
        "compiler": "babel",
        "main": "apps/acf-extension/src/main.ts",
        "tsConfig": "apps/acf-extension/tsconfig.app.json",
        "webpackConfig": "apps/acf-extension/webpack.config.js",
        "assets": ["apps/acf-extension/src/favicon.ico", "apps/acf-extension/src/assets"],
        "index": "apps/acf-extension/src/index.html",
        "baseHref": "/",
        "styles": ["apps/acf-extension/src/styles.css"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "fileReplacements": [
            {
              "replace": "apps/acf-extension/src/environments/environment.ts",
              "with": "apps/acf-extension/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "options": {
        "buildTarget": "acf-extension:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "acf-extension:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/acf-extension/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/acf-extension/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  }
}
