{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "dist",
    "types": ["chrome", "node", "@nx/react/typings/cssmodule.d.ts", "@nx/react/typings/image.d.ts", "vite/client", "vitest/importMeta"],
    "jsx": "react-jsx",
    "rootDir": "src",
    "tsBuildInfoFile": "dist/tsconfig.app.tsbuildinfo",
    "lib": ["dom"],
    "baseUrl": ".",
    "paths": {
      "@acf-options-page/*": ["src/*"]
    }
  },
  "exclude": [
    "out-tsc",
    "dist",
    "src/**/*.spec.ts",
    "src/**/*.test.ts",
    "src/**/*.spec.tsx",
    "src/**/*.test.tsx",
    "src/**/*.spec.js",
    "src/**/*.test.js",
    "src/**/*.spec.jsx",
    "src/**/*.test.jsx",
    "eslint.config.js",
    "eslint.config.cjs",
    "eslint.config.mjs"
  ],
  "include": ["src/**/*.js", "src/**/*.jsx", "src/**/*.ts", "src/**/*.tsx", "../../packages/core/open-telemetry/src/lib/open-telemetry-web.ts"],
  "references": [
    {
      "path": "../../packages/shared/firebase-firestore/tsconfig.lib.json"
    },
    {
      "path": "../../packages/shared/discord-oauth/tsconfig.lib.json"
    },
    {
      "path": "../../packages/shared/firebase-oauth/tsconfig.lib.json"
    },
    {
      "path": "../../packages/acf/service/tsconfig.lib.json"
    },
    {
      "path": "../../packages/shared/google-analytics/tsconfig.lib.json"
    },
    {
      "path": "../../packages/shared/google-oauth/tsconfig.lib.json"
    },
    {
      "path": "../../packages/shared/google-drive/tsconfig.lib.json"
    },
    {
      "path": "../../packages/shared/status-bar/tsconfig.lib.json"
    },
    {
      "path": "../../packages/core/open-telemetry/tsconfig.lib.json"
    },
    {
      "path": "../../packages/core/service/tsconfig.lib.json"
    },
    {
      "path": "../../packages/core/common/tsconfig.lib.json"
    },
    {
      "path": "../../packages/acf/common/tsconfig.lib.json"
    }
  ]
}
