{
  "name": "@dhruv-techapps/shared-google-sheets",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@dhruv-techapps/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./service": {
      "types": "./dist/google-sheets.service.d.ts",
      "import": "./dist/google-sheets.service.js",
      "default": "./dist/google-sheets.service.js"
    },
    "./content_script": {
      "types": "./dist/google-sheets-content_script.d.ts",
      "import": "./dist/google-sheets-content_script.js",
      "default": "./dist/google-sheets-content_script.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "@swc/helpers": "~0.5.11",
    "@dhruv-techapps/shared-firebase-functions": "0.0.1",
    "@dhruv-techapps/shared-google-oauth": "0.0.1",
    "@dhruv-techapps/shared-notifications": "0.0.1",
    "@dhruv-techapps/core-common": "0.0.1",
    "@dhruv-techapps/core-service": "0.0.1"
  }
}
