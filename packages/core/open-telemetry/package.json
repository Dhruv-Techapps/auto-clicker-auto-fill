{
  "name": "@dhruv-techapps/core-open-telemetry",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@dhruv-techapps/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./options-page": {
      "types": "./dist/options-page.d.ts",
      "import": "./dist/options-page.js",
      "default": "./dist/options-page.js"
    },
    "./content-script": {
      "types": "./dist/content-script.d.ts",
      "import": "./dist/content-script.js",
      "default": "./dist/content-script.js"
    },
    "./background": {
      "types": "./dist/background.d.ts",
      "import": "./dist/background.js",
      "default": "./dist/background.js"
    }
  },
  "nx": {
    "sourceRoot": "packages/core/open-telemetry/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "packages/core/open-telemetry/dist",
          "main": "packages/core/open-telemetry/src/index.ts",
          "tsConfig": "packages/core/open-telemetry/tsconfig.lib.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true
        }
      }
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "@swc/helpers": "~0.5.11",
    "@opentelemetry/resources": "2.2.0",
    "@opentelemetry/context-zone": "^2.2.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/instrumentation": "^0.208.0",
    "@opentelemetry/instrumentation-fetch": "0.208.0",
    "@opentelemetry/sdk-trace-base": "^2.2.0",
    "@opentelemetry/sdk-trace-web": "^2.2.0",
    "@opentelemetry/instrumentation-document-load": "0.54.0",
    "@opentelemetry/instrumentation-xml-http-request": "0.208.0",
    "@opentelemetry/exporter-logs-otlp-http": "^0.208.0",
    "@opentelemetry/core": "2.2.0",
    "@opentelemetry/sdk-logs": "^0.208.0",
    "@opentelemetry/api-logs": "^0.208.0",
    "@opentelemetry/api": "^1.9.0",
    "@dhruv-techapps/core-store": "0.0.1",
    "@opentelemetry/semantic-conventions": "1.38.0",
    "@opentelemetry/otlp-transformer": "0.208.0"
  }
}
