{
  "hosting": [
    {
      "site": "auto-clicker-autofill-configs",
      "public": "acf-configs",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "site": "auto-clicker-auto-fill-dev",
      "public": "dist/apps/acf-options-page",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ],
      "headers":[ {
        "source": "/**",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "https://auto-clicker-auto-fill-dev.web.app"
          },
          {
            "key": "Access-Control-Allow-Origin",
            "value": "https://auto-clicker-auto-fill-dev.firebaseapp.com"
          },
          {
            "key": "Access-Control-Allow-Origin",
            "value": "https://dev.getautoclicker.com"
          }
        ]
      }]
    },
    {
      "site": "auto-clicker-auto-fill-beta",
      "public": "dist/apps/acf-options-page",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "site": "auto-clicker-auto-fill-prod",
      "public": "dist/apps/acf-options-page",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ]
}
